package prak2;
import java.util.ArrayList;

public class Fuehrung {
	
	ArrayList<Arbeitskraft> anfuehrer = new ArrayList<Arbeitskraft>();
	

	private String name;
	private int personalnummer;
	private String kompetenz;
	private Arbeitskraft arbeiter;

	public String getName() {
		return name;
	}
	public int getPersonalnummer() {
		return personalnummer;
	}
	public String getKompetenz() {
		return kompetenz;
	}

	public void setName(String name) {
		this.name = name;
	}

	public void setPersonalnummer(int personalnummer) {
		this.personalnummer = personalnummer;
	}
	
	public void setKompetenz(String kompetenz) {
		this.kompetenz = kompetenz;
	}
	

	Fuehrung(String name, int personalnummer, String kompetenz){
		this.setName(name);
		this.setPersonalnummer(personalnummer);
		this.setKompetenz(kompetenz);
	}
	//Wie verbinde ich Arbeitskraft mit Fuehrung?
	//public void zuweisungInAbteilung(Arbeitskraft arbeiter) {
//		System.out.println(arbeiter + " Sie wurden in die Abteilung ");
	//}
		
	
}
